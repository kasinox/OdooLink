<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.input.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="144.0" prefWidth="800.0" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.alltekusa.qbLink.Viewer.Console.GUI.MainControllerGUI">
         <top>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <MenuBar>
                    <menus>
                      <Menu mnemonicParsing="false" text="File">
                        <items>
                          <MenuItem id="closeButton" mnemonicParsing="true" text="Close">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="X" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator></MenuItem>
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Edit">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Delete" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Help">
                        <items>
                          <MenuItem mnemonicParsing="false" text="About" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
                  <HBox spacing="3.0">
                     <children>
                        <Label prefHeight="21.0" prefWidth="61.0" text="Choose Function">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Label>
                  <ChoiceBox fx:id="searchChoiceBox" prefHeight="25.0" prefWidth="121.0" />
                        <TextField fx:id="searchTextField" prefHeight="25.0" prefWidth="369.0" promptText="Enter search here">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </TextField>
                        <Button fx:id="searchButton" mnemonicParsing="false" onAction="#search" text="Confirm">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="Advanced">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </padding>
                  </HBox>
            <HBox spacing="3.0">
               <children>
                  <Button fx:id="connectButton" mnemonicParsing="false" onAction="#QBConnection" text="Quickbooks Connect " />
                  <Button fx:id="disconnectButton" disable="true" mnemonicParsing="false" onAction="#QBDisconnect" text="Disconnect" />
               </children>
               <VBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </HBox>
               </children>
            </VBox>
         </top>
   <left>
      <HBox prefHeight="245.0" prefWidth="553.0" BorderPane.alignment="CENTER" />
   </left>
   <bottom>
            <Label fx:id="statusbar" text="Status:" BorderPane.alignment="CENTER"/>
   </bottom>
     
 </fx:root>
